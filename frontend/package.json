{
  "name": "diversity-frontend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\" > build/prettier-result.txt 2>&1",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "watch:prod": "webpack --config webpack.prod.js --watch"
  },
  "author": "",
  "dependencies": {
    "gsap": "3.3.3",
    "jquery": "3.5.1",
    "stimulus": "1.1.1",
    "swiper": "4.5.0"
  },
  "devDependencies": {
    "@types/gsap": "1.20.2",
    "@types/jquery": "3.5.1",
    "@types/swiper": "5.4.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.3.0",
    "fibers": "5.0.0",
    "ignore-emit-webpack-plugin": "2.0.3",
    "mini-css-extract-plugin": "0.11.2",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "prettier": "2.1.1",
    "sass": "1.26.10",
    "sass-loader": "10.0.2",
    "ts-loader": "8.0.3",
    "typescript": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-merge": "5.1.4"
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
