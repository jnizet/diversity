{
  "name": "diversity-frontend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\" > build/prettier-result.txt 2>&1",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "watch:prod": "webpack --config webpack.prod.js --watch"
  },
  "author": "",
  "dependencies": {
    "gsap": "3.5.1",
    "jquery": "3.5.1",
    "stimulus": "1.1.1",
    "swiper": "6.3.4"
  },
  "devDependencies": {
    "@types/gsap": "1.20.2",
    "@types/jquery": "3.5.1",
    "@types/swiper": "5.4.0",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "9.2.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "ignore-emit-webpack-plugin": "2.0.6",
    "mini-css-extract-plugin": "2.6.0",
    "prettier": "2.6.2",
    "sass": "1.51.0",
    "sass-loader": "12.6.0",
    "ts-loader": "9.3.0",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
