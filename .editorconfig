# Editor configuration, see https://editorconfig.org
root = true

[*]
indent_size = 2
charset = utf-8
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[{*.ts, *.js, *.scss}]
quote_type = single

[{*.java, *.kts}]
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = false

